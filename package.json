{
  "name": "automation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "node cypress/reports/setup/delete-results.js",
    "test": "npx cypress run",
    "cy:openPartial": "npx cypress open --config-file cypress_partial.config.js",
    "test_partial": "npx cypress run --config-file cypress_partial.config.js",
    "test_tag": "npx cypress-tags run --env TAGS=\"@smoke\"",
    "test_smoke1": "npx cypress run --env TAGS=\"@smoke and not (@skip or @UnderFix)\"",
    "//Details": "echo 'Below Set for Parallel Regression run'",
    "test_partial:Set1": "npx cypress run --config-file cypressSet1.config.js --env TAGS=\"not (@skip or @UnderFix)\" --headless ",
    "test_partial:Set2": "npx cypress run --config-file cypressSet2.config.js --env TAGS=\"not (@skip or @UnderFix)\"  --headless ",
    "test_runParallel1": "run-p -c -l -n test_partial:Set*",
    "report_gen": "node cypress/reports/setup/reportOfflineGeneration.js",
    "SendMail": "node cypress/reports/setup/sendEmailManually.js",
    "test_reg:Parallel": "npm-run-all -s -c test_runParallel1 report_gen SendMail",
    "//Details1": "echo 'Below Set for Parallel Smoke run'",
    "test_partialSmoke:Set1": "npx cypress-tags run --config-file cypressSet1.config.js --env TAGS=\"@smoke\",grep=\"smoke\",grepFilterSpecs=true,grepOmitFiltered=true",
    "test_partialSmoke:Set2": "npx cypress-tags run --config-file cypressSet2.config.js --env TAGS=\"@smoke\",grep=\"smoke\",grepFilterSpecs=true,grepOmitFiltered=true",
    "test_runParallel2": "run-p -c -l -n test_partialSmoke:*",
    "test_Smoke:Parallel": "npm-run-all -s -c test_runParallel2 report_gen SendMail",
    "//LiveComments": "echo 'Below Set for Live daily run of smoke and reg'",
    "test_smoke": "npx cypress run --env TAGS=\"@smoke and not (@skip or @UnderFix)\" --headless  ",
    "test_smokeWithRec": "npx cypress-tags run --config-file cypress.config.js --env TAGS=\"@smoke and not (@skip or @UnderFix)\",grep=\"smoke\",grepFilterSpecs=true,grepOmitFiltered=true --config video=true",
    "test_reg_backup": "npx cypress run --env TAGS=\"not (@skip or @UnderFix)\"",
    "test_reg": "npx cypress run --env TAGS=\"not (@skip or @UnderFix)\" --headless ",
    "test_reg:record": "npx cypress run --env TAGS=\"not (@skip or @UnderFix)\" --record --key c0360273-8cb0-4501-a1de-b19661db6f54",
    "//CommentsWithAllure": "echo '=====Allure Related commands ======'",
    "test_reg:withAllure": "npx cypress run --env allure=true,allureResultsPath=../results/cypress/screenshots/allure-results,TAGS=\"not (@skip or @UnderFix)\"",
    "test_reg:allure": "npm-run-all -s -c test_reg:withAllure report:allureGenerate",
    "test_partial:withAllure": "npx cypress run --config-file cypress_partial.config.js --env allure=true,allureResultsPath=../results/cypress/screenshots/allure-results,TAGS=\"not (@skip or @UnderFix)\"",
    "test_partial:allure": "npm-run-all -s -c test_partial:withAllure report:allureGenerate",
    "//Details2": "echo 'Below Set for Parallel Regression run with Allure'",
    "test_partial:SetAllure1": "npm run pretest && npx cypress run --config-file cypressSet1.config.js --headless --browser chrome --env allure=true,allureResultsPath=../results/cypress/screenshots/allure-results,TAGS=\"not (@skip or @UnderFix)\"",
    "test_partial:SetAllure2": "npm run pretest && npx cypress run --config-file cypressSet2.config.js --headless --browser chrome --env allure=true,allureResultsPath=../results/cypress/screenshots/allure-results,TAGS=\"not (@skip or @UnderFix)\"",
    "test_runParallel3": "run-p -c -l -n test_partial:SetAllure*",
    "test_reg:ParallelWithAllure": "npm-run-all -s -c test_runParallel3 report_gen SendMail report:allureGenerate",
    "report:allureGenerate": "npx allure generate \"../results/cypress/screenshots/allure-results\" --clean -o \"../results/cypress/screenshots/allure-report\"",
    "report:allureOpen": "npx allure open \"../results/cypress/screenshots/allure-report\""
  },
  "author": "triveni",
  "license": "ISC",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.1",
    "@bahmutov/cy-api": "^2.2.1",
    "@shelex/cypress-allure-plugin": "^2.33.1",
    "@testing-library/cypress": "^8.0.3",
    "allure-commandline": "^2.18.1",
    "chai-string": "^1.5.0",
    "chalk": "^4.1.2",
    "cucumber-html-reporter": "^5.5.0",
    "cypress": "^10.11.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-grep": "^3.0.2",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-real-events": "^1.7.1",
    "cypress-wait-until": "^1.7.2",
    "cypress-xpath": "^2.0.1",
    "del": "^6.1.1",
    "eslint-plugin-cypress": "^2.12.1",
    "gmail-tester-extended": "^1.1.2",
    "html-to-text": "^8.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "mongodb": "^4.11.0",
    "nodemailer": "^6.8.0",
    "npm": "^8.19.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "../results/cypress/reports/test-results/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  }
}
